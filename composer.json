{
  "name": "youwe/pimcore-deployment",
  "type": "pimcore-plugin",
  "description": "Youwe Pimcore Deployment",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.youwe.nl"
    },
    {
      "type": "git",
      "url": "ssh://git@source.youwe.nl:7999/pimb2b/deployment.git"
    }
  ],
  "autoload": {
    "psr-0": {"Pimcore\\Composer\\": "src/"}
  },
  "extra": {
    "class": "Pimcore\\Composer\\PluginInstaller"
  },
  "require": {
    "pimcore/installer-plugin": "^1.3"
  }
}
